type Query{
    employees: [Employee]
}

type Employee{
 id: String!,
 name: String!,
 dateOfBirth: String!,
 city: String!,
 salary: Float!,
 gender: String!,
 email: String!
}

input CreateEmployeeInput{
  name: String,
  dateOfBirth: String,
  city: String,
  salary: Float,
  gender: String,
  email: String
}

type Mutation {
 newEmployee(createEmployeeInput: CreateEmployeeInput!): Employee!
}